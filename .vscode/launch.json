  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: very verbose",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": { "PYTHONPATH": "${workspaceRoot}" },
      "args": ["-vv"]
    },
    {
      "name": "Python: verbose",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": { "PYTHONPATH": "${workspaceRoot}" },
      "args": ["-v"]
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "env": { "PYTHONPATH": "${workspaceRoot}" }
    }
  ]
}
